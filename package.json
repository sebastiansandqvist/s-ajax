{
  "name": "s-ajax",
  "version": "0.0.1",
  "description": "Dependency-free AJAX requests",
  "main": "index.js",
  "directories": {
    "test": "test"
  },
  "scripts": {
    "test": "mocha",
    "lint": "jscs index.js --config=./etc/.jscs.json && jshint index.js",
    "coverage_html": "mocha --require blanket -R html-cov > etc/coverage.html",
    "coverage": "mocha --require blanket --reporter mocha-lcov-reporter > etc/lcov.info"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/sebastiansandqvist/s-ajax.git"
  },
  "keywords": [
    "ajax",
    "client",
    "frontend",
    "javascript",
    "xhr",
    "request"
  ],
  "author": "sebastian sandqvist",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/sebastiansandqvist/s-ajax/issues"
  },
  "homepage": "https://github.com/sebastiansandqvist/s-ajax",
  "devDependencies": {
    "blanket": "1.1.6",
    "chai": "^2.3.0",
    "codeclimate-test-reporter": "0.0.4",
    "jscs": "^1.13.1",
    "jshint": "^2.8.0",
    "mocha": "^2.2.4",
    "mocha-lcov-reporter": "0.0.2"
  }
}
